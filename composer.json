{
    "name": "foa/auth-session-bundle",
    "description": "Integration of Aura.Auth and Aura.Session",
    "keywords": [
        "auth",
        "authentication",
        "session"
    ],
    "type": "library",
    "require": {
        "aura/auth": "~2.0",
        "aura/session": "~2.0",
        "ircmaxell/random-lib": "~1.0"
    },
    "require-dev": {
        "aura/di": "~2.1",
        "phpunit/phpunit": "~5.7 || ~4.8"
    },
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Hari KT",
            "email": "kthari85@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "FOA\\Auth_Session_Bundle\\": "src/",
            "FOA\\Auth_Session_Bundle\\_Config\\": "config/"
        }
    },
    "extra": {
        "aura": {
            "type": "bundle",
            "config": {
                "common": "FOA\\Auth_Session_Bundle\\_Config\\Common"
            }
        }
    }
}
